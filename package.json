{
  "name": "nestjs-project-builder",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pipeline:ci": "npm run build && npm run lint && npm run good:fences",
    "migration:generate": "npx typeorm -f dist/application/config/database.config.js -d src/infrastructure/database/migrations migration:generate -n",
    "migration:run": "npx typeorm -f dist/application/config/database.config.js migration:run",
    "migration:revert": "npx typeorm -f dist/application/config/database.config.js migration:revert",
    "db:drop": "npx typeorm -f dist/application/config/database.config.js schema:drop",
    "fixtures:load": "npx fixtures ./src/infrastructure/database/fixtures -c ./dist/application/config/database.config.js",
    "fixtures:load_new": "npx fixtures ./src/infrastructure/database/fixtures -c dist/application/config/database.config.js",
    "seeder:seed": "node dist/database/seeder/seeder.js",
    "db:recreate": "yarn db:drop && yarn migration:run && yarn fixtures:load",
    "good:fences": "gf",
    "ci:flow": "npm run build && npm run good:fences"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "^0.1.4",
    "@nest-toolbox/winston-logger": "^1.3.1",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs-query/core": "^0.22.0",
    "@nestjs-query/query-graphql": "^0.22.0",
    "@nestjs-query/query-typeorm": "^0.22.0",
    "@nestjs/common": "^7.6.12",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/platform-socket.io": "^7.6.12",
    "@nestjs/typeorm": "^7.1.5",
    "@slynova/flydrive": "^1.0.3",
    "apollo-server-express": "^2.21.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.2.0",
    "mysql2": "^2.2.5",
    "nest-schedule": "^0.6.4",
    "nest-status-monitor": "^0.1.4",
    "nestjs-enlighten": "0.0.8-beta",
    "nodemailer": "^6.4.18",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "good-fences": "^0.9.2",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "tsconfig-paths": "^3.9.0",
    "typeorm-fixtures-cli": "^1.9.1",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@application": "dist/application",
    "@infrastructure": "dist/infrastructure",
    "@domain": "dist/domain",
    "@api": "dist/infrastructure/api",
    "@enum": "dist/domain/enum"
  }
}
